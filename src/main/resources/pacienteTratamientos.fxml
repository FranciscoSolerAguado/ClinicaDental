<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.PacienteTratamientoController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="10.0" prefWidth="600.0" prefHeight="400.0" AnchorPane.topAnchor="20.0">
        <Label text="Tratamientos del Paciente" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
        <ListView fx:id="tratamientosListView" prefHeight="300.0" prefWidth="500.0"/>
        <Button text="Cerrar" onAction="#cerrarVentana" style="-fx-background-color: #F44336; -fx-text-fill: white;"/>
    </VBox>
</AnchorPane>
